{
  "headers": [
    {
      "source": "/(.*)",
      "headers" : [
        {
          "key": "cache-control",
          "value": "public, max-age=3600, must-revalidate"
        }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers" : [
        {
          "key": "referer",
          "value": "https://docs.qq.com/resource/sheet/public-worker.0ffd8f13ed8dfcb8ae84.js?lng=zh-CN"
        },
        {
          "key": "cache-control",
          "value": "public, max-age=10, must-revalidate"
        }
      ],
      "has": [
        {
            "type": "host",
            "value": "https://docs.qq.com/sheet/DWmtpVWRlWEVXakpX?tab=BB08J2"
        }
      ]
    },
    {
      "source": "/doc/(.*)",
      "headers" : [
        {
          "key": "referer",
          "value": "https://docs.qq.com/resource/sheet/public-worker.0ffd8f13ed8dfcb8ae84.js?lng=zh-CN"
        },
        {
          "key": "cache-control",
          "value": "public, max-age=10, must-revalidate"
        }
      ],
      "has": [
        {
            "type": "host",
            "value": "https://docs.qq.com/sheet/DWmtpVWRlWEVXakpX?tab=BB08J2"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/proxy"
    },
    {
      "source": "/doc/(.*)",
      "destination": "/api/excel"
    }
  ]
}

